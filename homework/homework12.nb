Cell[TextData[{
 StyleBox["Problem 6\na) ",
  FontWeight->"Bold"],
 "Plot the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{"-", "x"}]], "/", 
     RowBox[{"(", 
      RowBox[{"2", "+", 
       RowBox[{"sin", "(", 
