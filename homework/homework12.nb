    RowBox[{"f", "(", "x", ")"}], "-", 
    RowBox[{"g", "(", "x", ")"}]}], TraditionalForm]],ExpressionUUID->
  "df9ebd8f-9ac0-4a5b-921f-77dfd865ea84"],
