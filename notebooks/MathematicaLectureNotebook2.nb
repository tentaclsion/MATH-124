 CellLabel->
  "In[182]:=",ExpressionUUID->"7a2aab41-6440-45c9-b47f-5f9591731e6f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Timing", "[", 
   RowBox[{"Select", "[", 
    RowBox[{"mylist", ",", 
     RowBox[{
      RowBox[{
       RowBox[{"Mod", "[", 
        RowBox[{"#", ",", "2"}], "]"}], "\[Equal]", "0"}], "&"}]}], "]"}], 
   "]"}], "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.76398292888244*^9, 3.7639829573936987`*^9}, {
